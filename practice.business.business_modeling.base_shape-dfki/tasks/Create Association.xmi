<org.eclipse.epf.uma:TaskDescription xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:org.eclipse.epf.uma="http://www.eclipse.org/epf/uma/1.0.6/uma.ecore" xmlns:epf="http://www.eclipse.org/epf" epf:version="1.5.1" xmi:id="--XvUgm6R8MIxvQvxd6pulA" name="Association_Modelling,_lPvaoERmEd6rK5RiDPV2HA" guid="--XvUgm6R8MIxvQvxd6pulA" changeDate="2009-05-21T17:56:08.202+0200">
  <mainDescription>&lt;p align=&quot;left&quot;>&#xD;
    &lt;font face=&quot;Arial&quot; size=&quot;2&quot;>&lt;font face=&quot;Arial&quot; size=&quot;2&quot;>An Association is used to associate information with Flow&#xD;
    Objects. Text and graphical non-Flow Objects can be associated with the Flow Objects.&lt;/font>&lt;/font>&#xD;
&lt;/p></mainDescription>
  <sections xmi:id="_4rU6kEYcEd6LIJ4mp_lkVg" name="Create a new association object" guid="_4rU6kEYcEd6LIJ4mp_lkVg">
    <sectionDescription>&lt;p>&#xD;
    Choose the association object of your choice out of the toolbar.&#xD;
&lt;/p>&#xD;
&lt;p>&#xD;
    Choose your start element and press(hold) the left mouse button.&#xD;
&lt;/p>&#xD;
&lt;p>&#xD;
    Now move the mouse to your destination element and after that let loose the mouse button.&#xD;
&lt;/p></sectionDescription>
  </sections>
  <sections xmi:id="_BJsYkEYeEd6LIJ4mp_lkVg" name="Name the association object" guid="_BJsYkEYeEd6LIJ4mp_lkVg">
    <sectionDescription>&lt;p>&#xD;
    Click on the association object and name the association object under properities.&#xD;
&lt;/p></sectionDescription>
  </sections>
</org.eclipse.epf.uma:TaskDescription>
