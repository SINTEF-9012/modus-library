  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-xQGBWnwzZNrB77jHbeJPlw" name="Create Attribute,_JKsnlTwkEd-qjbwc3TjCdQ" guid="-xQGBWnwzZNrB77jHbeJPlw">
    <refinedDescription>&lt;p>&#xD;
    Attributes are&amp;nbsp;the data elements, such as numbers, strings, or date values, that describe the state and contents&#xD;
    of a BPMN Object.&#xD;
&lt;/p></refinedDescription>
  </org.eclipse.epf.uma:DescriptorDescription>
