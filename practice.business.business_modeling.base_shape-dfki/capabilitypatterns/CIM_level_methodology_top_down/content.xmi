    &lt;font face=&quot;Arial&quot; size=&quot;2&quot;>&lt;font face=&quot;Arial&quot; size=&quot;2&quot;>Data Objects provide information about what activities require&#xD;
    to be performed and/or what they produce.&lt;/font>&lt;/font>&#xD;
&lt;/p></refinedDescription>
  </org.eclipse.epf.uma:DescriptorDescription>
  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-9CTnyUsd3hA0e3Q7kMYIdw" name="Analyst,_JKsnljwkEd-qjbwc3TjCdQ" guid="-9CTnyUsd3hA0e3Q7kMYIdw">
