  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-eoGQL6f8WbtGNuI_lOTfsg" name="User,_btRx9zwkEd-qjbwc3TjCdQ" guid="-eoGQL6f8WbtGNuI_lOTfsg">
