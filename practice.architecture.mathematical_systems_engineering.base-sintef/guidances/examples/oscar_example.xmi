&lt;P style=&quot;MARGIN: 0cm 0cm 0pt&quot; class=MsoNoSpacing>&lt;SPAN style=&quot;COLOR: #1f497d; mso-themecolor: text2&quot;>&lt;FONT size=3>&lt;FONT face=Calibri>&lt;SPAN style=&quot;mso-tab-count: 1&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>C_Star = 4450 * exp( ((-0.4) * log(v)) );&lt;o:p>&lt;/o:p>&lt;/FONT>&lt;/FONT>&lt;/SPAN>&lt;/P>&#xD;
&lt;P style=&quot;MARGIN: 0cm 0cm 0pt&quot; class=MsoNoSpacing>&lt;SPAN style=&quot;COLOR: #1f497d; mso-themecolor: text2&quot;>&lt;FONT size=3>&lt;FONT face=Calibri>&lt;SPAN style=&quot;mso-tab-count: 1&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>D = 0.0034 * Pw * g * Hb * Hb;&lt;o:p>&lt;/o:p>&lt;/FONT>&lt;/FONT>&lt;/SPAN>&lt;/P>&#xD;
&lt;P style=&quot;MARGIN: 0cm 0cm 0pt&quot; class=MsoNoSpacing>&lt;SPAN style=&quot;COLOR: #1f497d; mso-themecolor: text2&quot;>&lt;FONT size=3>&lt;FONT face=Calibri>&lt;SPAN style=&quot;mso-tab-count: 1&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>F = 0.000003 * exp( ( 3.5 * log(U_t)) );&lt;o:p>&lt;/o:p>&lt;/FONT>&lt;/FONT>&lt;/SPAN>&lt;/P>&#xD;
&lt;P style=&quot;MARGIN: 0cm 0cm 0pt&quot; class=MsoNoSpacing>&lt;SPAN style=&quot;COLOR: #1f497d; mso-themecolor: text2&quot;>&lt;FONT size=3>&lt;FONT face=Calibri>&lt;SPAN style=&quot;mso-tab-count: 1&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>di = ( Co * exp( (0.34 * log(v)) ) ) / ( sqrt(e));&lt;o:p>&lt;/o:p>&lt;/FONT>&lt;/FONT>&lt;/SPAN>&lt;/P>&#xD;
&lt;P style=&quot;MARGIN: 0cm 0cm 0pt&quot; class=MsoNoSpacing>&lt;SPAN style=&quot;COLOR: #1f497d; mso-themecolor: text2&quot;>&lt;FONT size=3>&lt;FONT face=Calibri>&lt;SPAN style=&quot;mso-tab-count: 1&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>Qdi = C_Star * exp( (0.57* log(D))) * S * F * exp( (0.7 * log(di)) ) * do;&lt;o:p>&lt;/o:p>&lt;/FONT>&lt;/FONT>&lt;/SPAN>&lt;/P>&#xD;
