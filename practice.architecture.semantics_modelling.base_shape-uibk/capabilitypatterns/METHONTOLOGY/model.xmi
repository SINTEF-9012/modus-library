    </processElements>
    <processElements xsi:type="org.eclipse.epf.uma:TaskDescriptor" xmi:id="_nhBuck7zEd65M-yHtGQz8g" name="Evaluation" guid="_nhBuck7zEd65M-yHtGQz8g" presentationName="Evaluation" briefDescription="Technically judge the quality of the ontology." superActivities="_lUEHoU7zEd65M-yHtGQz8g" isRepeatable="true" additionallyPerformedBy="_nhBueE7zEd65M-yHtGQz8g" mandatoryInput="_nhBue07zEd65M-yHtGQz8g" output="_JBfwMfqlEd68-aqICre0mw" performedPrimarilyBy="_JBfwMPqlEd68-aqICre0mw">
