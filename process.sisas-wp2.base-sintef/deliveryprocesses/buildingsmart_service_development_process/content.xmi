  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-yUnr1SQHvrZIq9RExYXOhw" name="system_architect,_cq3jERQ7EeGgOd2_jEsctg" guid="-yUnr1SQHvrZIq9RExYXOhw"/>
