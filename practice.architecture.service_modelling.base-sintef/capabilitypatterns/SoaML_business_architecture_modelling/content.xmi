  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-nqB9Z_Pe6GNBg6bDOiIpxA" name="business_information_model,_Oo-ZAaGlEeC1A-PuG6Rlow" guid="-nqB9Z_Pe6GNBg6bDOiIpxA">
    <refinedDescription>&lt;p>&#xD;
