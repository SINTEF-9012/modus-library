  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-3lXuAxTG7J4Nu10R4UauFg" name="system_architect,_xsmjsUNGEeCJGq_wdQ8Swg" guid="-3lXuAxTG7J4Nu10R4UauFg">
