  <org.eclipse.epf.uma:ProcessDescription xmi:id="-rkJAzKwrtaqFDrWYrOg9_Q" name="adaptation_modelling,_BEBMkQeREeCk4NIWYgM-Lg" guid="-rkJAzKwrtaqFDrWYrOg9_Q">
