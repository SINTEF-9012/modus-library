  <org.eclipse.epf.uma:ProcessDescription xmi:id="-HPrEUTl-_zuNK5eaJkqzmA" name="adaptation_architecture_modelling,_qu_ugQeUEeCk4NIWYgM-Lg" guid="-HPrEUTl-_zuNK5eaJkqzmA">
