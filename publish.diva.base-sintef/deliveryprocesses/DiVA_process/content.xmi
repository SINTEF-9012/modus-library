  </org.eclipse.epf.uma:ProcessDescription>
  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-Skx5bVnhCZ19J9M2BQaPyw" name="system_developer,_01f9EQeXEeCk4NIWYgM-Lg" guid="-Skx5bVnhCZ19J9M2BQaPyw">
