  </org.eclipse.epf.uma:DescriptorDescription>
  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-9HIgyAfkpzuNOyFfYbIzCg" name="performance,_RBpxUFPCEeGZlfB-Ol6wYA" guid="-9HIgyAfkpzuNOyFfYbIzCg">
