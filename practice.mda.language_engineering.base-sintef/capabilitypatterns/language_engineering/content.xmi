  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-9YSem14A8Tw_oFHWj-k44Q" name="dsl_action_semantics,_sGmIEVYLEeCmC7fuNTHhhQ" guid="-9YSem14A8Tw_oFHWj-k44Q">
