  </org.eclipse.epf.uma:DescriptorDescription>
  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-wU4ChdZCQxOlvDbloHV3uw" name="dsl_domain_model,_sGah4VYLEeCmC7fuNTHhhQ" guid="-wU4ChdZCQxOlvDbloHV3uw">
    <refinedDescription>Specification of the domain model or conceptual model of the domain that the modelling language should cover.</refinedDescription>
