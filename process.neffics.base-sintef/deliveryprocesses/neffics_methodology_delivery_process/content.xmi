  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-4D_4b4Eok7IOEzW_3fMEKg" name="software_components,_-VLwUFOvEeGcEMRXpnhBww" guid="-4D_4b4Eok7IOEzW_3fMEKg">
