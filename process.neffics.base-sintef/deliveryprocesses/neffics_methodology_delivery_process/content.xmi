  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-GIIdR7jcjkAUPzTxKGLJwg" name="interfaces_and_messages,_-VIF8VOvEeGcEMRXpnhBww" guid="-GIIdR7jcjkAUPzTxKGLJwg">
