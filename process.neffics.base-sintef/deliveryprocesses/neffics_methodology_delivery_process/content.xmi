  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-RQEeCzS-tiBPX1-xNdJDAw" name="business_information_model,_83Zl0aJJEeC6g8wb3xknIg" guid="-RQEeCzS-tiBPX1-xNdJDAw">
