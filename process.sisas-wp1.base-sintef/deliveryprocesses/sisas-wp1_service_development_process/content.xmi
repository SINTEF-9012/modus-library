  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-13YYrqfm7qmQiPF05bYVEw" name="community_services_architecture,_o3_6Ib0OEeCaJPhuvOwBFQ" guid="-13YYrqfm7qmQiPF05bYVEw"/>
  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-mvbJG1RCC9Tsjr_ozmGJDA" name="participant_services_architecture,_o4AhMb0OEeCaJPhuvOwBFQ" guid="-mvbJG1RCC9Tsjr_ozmGJDA"/>
  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-B8unxEgYQsq8-oHFwa7kRg" name="requirement_diagram_and_use_case_diagram,_o4BvUb0OEeCaJPhuvOwBFQ" guid="-B8unxEgYQsq8-oHFwa7kRg"/>
